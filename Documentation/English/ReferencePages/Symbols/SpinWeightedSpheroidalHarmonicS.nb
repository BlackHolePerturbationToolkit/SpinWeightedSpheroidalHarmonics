Cell[TextData[{
 Cell[BoxData[
  ButtonBox["SpinWeightedSpheroidalHarmonicS",
   BaseStyle->"Link",
   ButtonData->
    "paclet:SpinWeightedSpheroidalHarmonics/ref/\
SpinWeightedSpheroidalHarmonicS"]], "InlineFormula",ExpressionUUID->
  "29b98934-8907-4b5c-8cf3-099937827060"],
 " satisfies the spin-weighted spheroidal equation ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"[", 
           RowBox[{
             FractionBox["1", 
              RowBox[{"sin", "\[Theta]"}]], 
             FractionBox[
              StyleBox["d", "TI"], 
              RowBox[{
                StyleBox["d", "TI"], "\[Theta]"}]], 
             RowBox[{"(", 
               RowBox[{"sin", "\[Theta]", 
                 FractionBox[
                  StyleBox["d", "TI"], 
                  RowBox[{
                    StyleBox["d", "TI"], "\[Theta]"}]]}], ")"}], "-", 
             SuperscriptBox["\[Gamma]", "2"], 
             SuperscriptBox["sin", "2"], "\[Theta]", "-", 
             FractionBox[
              SuperscriptBox[
               RowBox[{"(", 
                 RowBox[{
                   StyleBox["m", "TI"], "+", 
                   StyleBox["s", "TI"], "cos", "\[Theta]"}], ")"}], "2"], 
              RowBox[{
                SuperscriptBox["sin", "2"], "\[Theta]"}]], "-", "2", 
             "\[Gamma]", 
             StyleBox["s", "TI"], "cos", "\[Theta]", "+", 
             StyleBox["s", "TI"], "+", "2", 
             StyleBox["m", "TI"], "\[Gamma]", "+", 
             SubscriptBox["\[Null]", 
              StyleBox["s", "TI"]], 
             SubscriptBox["\[Lambda]", 
              RowBox[{
                StyleBox["l", "TI"], 
                StyleBox["m", "TI"]}]]}], "]"}], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox[
          StyleBox["S", "TI"], 
          RowBox[{
            StyleBox["l", "TI"], 
            StyleBox["m", "TI"]}]], "\[LongEqual]", "0"}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\left[\\frac{1}{\\sin\\theta} \\frac{d}{d\\theta} \\left( \\sin\\theta \
\\frac{d}{d\\theta}\\right) - \\gamma^2 \\sin^2\\theta - \
\\frac{(m+s\\cos\\theta)^2}{\\sin^2\\theta} - 2\\gamma s \\cos\\theta + s+ 2m\
\\gamma + {}_s\\lambda_{lm} \\right] { }_{s}S_{lm} = 0", "state" -> "Boxes"|>,
    
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e8e2a303-372c-41fa-a777-81876c028901"],
 "."
}], "Notes",
 CellChangeTimes->{{3.893617112605961*^9, 3.8936171249631577`*^9}, {
   3.8937511841661253`*^9, 3.893751184441874*^9}, {3.893782931040435*^9, 
