Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"s", "=", 
      RowBox[{"-", "2"}]}], ",", 
     RowBox[{"l", "=", "2"}], ",", 
     RowBox[{"m", "=", "2"}], ",", 
     RowBox[{"\[Gamma]", "=", "0.1"}]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"sSlm", "=", 
     RowBox[{"SpinWeightedSpheroidalHarmonicS", "[", 
      RowBox[{"s", ",", "l", ",", "m", ",", "\[Gamma]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"s\[Lambda]lm", "=", 
     RowBox[{"SpinWeightedSpheroidalEigenvalue", "[", 
      RowBox[{"s", ",", "l", ",", "m", ",", "\[Gamma]"}], "]"}]}], ";", 
    RowBox[{"Plot", "[", 
     RowBox[{
      RowBox[{"Evaluate", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           FractionBox["1", 
            RowBox[{"Sin", "[", "\[Theta]", "]"}]], 
           RowBox[{"D", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Sin", "[", "\[Theta]", "]"}], 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"sSlm", "[", 
                 RowBox[{"\[Theta]", ",", "0"}], "]"}], ",", "\[Theta]"}], 
               "]"}]}], ",", "\[Theta]"}], "]"}]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"-", 
               SuperscriptBox["\[Gamma]", "2"]}], 
              SuperscriptBox[
               RowBox[{"Sin", "[", "\[Theta]", "]"}], "2"]}], "-", 
             FractionBox[
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"m", "+", 
                 RowBox[{"s", " ", 
                  RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "2"], 
              SuperscriptBox[
               RowBox[{"Sin", "[", "\[Theta]", "]"}], "2"]], "-", 
             RowBox[{"2", "\[Gamma]", " ", "s", " ", 
              RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "+", "s", "+", 
             RowBox[{"2", "m", " ", "\[Gamma]"}]}], ")"}], 
           RowBox[{"sSlm", "[", 
            RowBox[{"\[Theta]", ",", "0"}], "]"}]}]}], ",", 
         RowBox[{
          RowBox[{"-", "s\[Lambda]lm"}], " ", 
          RowBox[{"sSlm", "[", 
           RowBox[{"\[Theta]", ",", "0"}], "]"}]}]}], "}"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}], ",", 
      RowBox[{"PlotStyle", "->", 
       RowBox[{"{", 
        RowBox[{"Automatic", ",", "Dashed"}], "}"}]}]}], "]"}]}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
 CellChangeTimes->{{3.8939129501189413`*^9, 3.893913203565419*^9}, {
  3.893913235942719*^9, 3.8939133750274277`*^9}, {3.893913483128559*^9, 
