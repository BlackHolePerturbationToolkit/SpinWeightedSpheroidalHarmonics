Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"s", "=", "2"}], ",", 
     RowBox[{"l", "=", "5"}], ",", 
     RowBox[{"m", "=", "3"}]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"sYlm", "=", 
     RowBox[{"FunctionExpand", "[", 
      RowBox[{
       StyleBox["SpinWeightedSphericalHarmonicY",
        FontColor->GrayLevel[0]], "[", 
       RowBox[{"s", ",", "l", ",", "m", ",", "\[Theta]", ",", "\[Phi]"}], 
       "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Lambda]", "=", 
     RowBox[{
      RowBox[{"l", 
       RowBox[{"(", 
        RowBox[{"l", "+", "1"}], ")"}]}], "-", 
      RowBox[{"s", 
       RowBox[{"(", 
        RowBox[{"s", "+", "1"}], ")"}]}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Simplify", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["1", 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]], 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Sin", "[", "\[Theta]", "]"}], 
           RowBox[{"D", "[", 
            RowBox[{"sYlm", ",", "\[Theta]"}], "]"}]}], ",", "\[Theta]"}], 
         "]"}]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           FractionBox[
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"m", "+", 
               RowBox[{"s", " ", 
                RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "2"], 
            SuperscriptBox[
             RowBox[{"Sin", "[", "\[Theta]", "]"}], "2"]]}], "+", "s"}], 
         ")"}], "sYlm"}]}], "==", 
      RowBox[{
       RowBox[{"-", "\[Lambda]"}], " ", "sYlm"}]}], "]"}]}]}], 
