Cell[TextData[{
 ButtonBox["SpinWeightedSphericalHarmonicY",
  BaseStyle->"Link",
  ButtonData->
   "paclet:SpinWeightedSpheroidalHarmonics/ref/\
SpinWeightedSphericalHarmonicY"],
 " satisfies the spin-weighted spherical equation, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"[", 
           RowBox[{
             FractionBox["1", 
              RowBox[{"sin", "\[Theta]"}]], 
             FractionBox[
              StyleBox["d", "TI"], 
              RowBox[{
                StyleBox["d", "TI"], "\[Theta]"}]], 
             RowBox[{"(", 
               RowBox[{"sin", "\[Theta]", 
                 FractionBox[
                  StyleBox["d", "TI"], 
                  RowBox[{
                    StyleBox["d", "TI"], "\[Theta]"}]]}], ")"}], "-", 
             FractionBox[
              SuperscriptBox[
               RowBox[{"(", 
                 RowBox[{
                   StyleBox["m", "TI"], "+", 
                   StyleBox["s", "TI"], "cos", "\[Theta]"}], ")"}], "2"], 
              RowBox[{
                SuperscriptBox["sin", "2"], "\[Theta]"}]], "+", 
             StyleBox["s", "TI"], "+", "\[Lambda]"}], "]"}], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox[
          StyleBox["Y", "TI"], 
          RowBox[{
            StyleBox["l", "TI"], 
            StyleBox["m", "TI"]}]], "\[LongEqual]", "0"}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\left[\\frac{1}{\\sin\\theta} \\frac{d}{d\\theta} \\left( \\sin\\theta \
\\frac{d}{d\\theta}\\right) - \\frac{(m+s\\cos\\theta)^2}{\\sin^2\\theta} + \
s+\\lambda\\right] { }_{s}Y_{lm} = 0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e5f361ba-8e44-4e5f-b7a0-0358a6fdec69"]
}], "Notes",
 CellChangeTimes->{{3.893956432317297*^9, 3.89395651540025*^9}, 
   3.8939565964600286`*^9},
 CellID->601332013,ExpressionUUID->"e7ca9140-8bf7-4929-9f02-2e2ab8f85368"],

Cell[TextData[{
 "The eigenvalue, ",
 Cell[BoxData["\[Lambda]"], "InlineFormula",
  FormatType->StandardForm,ExpressionUUID->
  "c1821768-ed1b-4a40-82c9-68fd87be4a8b"],
 " for the spin-weighted spherical equation is given by ",
 Cell[BoxData[
  RowBox[{"\[Lambda]", " ", "=", " ", 
   RowBox[{
    RowBox[{"l", 
     RowBox[{"(", 
      RowBox[{"l", "+", "1"}], ")"}]}], " ", "-", " ", 
    RowBox[{"s", 
     RowBox[{"(", 
      RowBox[{"s", "+", "1"}], ")"}]}]}]}]], "InlineFormula",
  FormatType->StandardForm,ExpressionUUID->
  "afe874c1-b94f-42a9-b2ee-ab1d273d55bb"],
 "."
}], "Notes",
 CellChangeTimes->{{3.893956479798276*^9, 3.8939564828415213`*^9}, {
  3.8939565205178013`*^9, 3.893956521245001*^9}},
 CellID->529099709,ExpressionUUID->"f95039eb-7462-440a-8e9b-4aa2761a0568"],

Cell[TextData[{
 ButtonBox["SpinWeightedSphericalHarmonicY",
  BaseStyle->"Link",
  ButtonData->
   "paclet:SpinWeightedSpheroidalHarmonics/ref/\
SpinWeightedSphericalHarmonicY"],
 " is unit-normalised on the two-sphere, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[Integral]", "0", 
          RowBox[{"2", "\[Pi]"}]], 
         SubsuperscriptBox["\[Integral]", "0", "\[Pi]"], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox[
          StyleBox["Y", "TI"], 
          RowBox[{
            StyleBox["l", "TI"], 
            StyleBox["m", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox[
          OverscriptBox[
           StyleBox["Y", "TI"], "_"], 
          RowBox[{
            SuperscriptBox[
             StyleBox["l", "TI"], "\[Prime]"], 
            SuperscriptBox[
             StyleBox["m", "TI"], "\[Prime]"]}]], 
         RowBox[{"(", 
           RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}], "\[DifferentialD]", 
         "\[Theta]", "\[DifferentialD]", "\[Phi]", "\[LongEqual]", 
         SubscriptBox["\[Delta]", 
          RowBox[{
            StyleBox["l", "TI"], 
            SuperscriptBox[
             StyleBox["l", "TI"], "\[Prime]"]}]], 
         SubscriptBox["\[Delta]", 
          RowBox[{
            StyleBox["m", "TI"], 
            SuperscriptBox[
             StyleBox["m", "TI"], "\[Prime]"]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\int_0^{2\\pi} \\int_0^\\pi {}_s Y_{l m}(\\theta, \\phi) {}_s {\\bar \
Y}_{l'm'}(\\theta, \\phi) \\, d\\theta \\, d\\phi = \\delta_{l l'} \\delta_{m \
m'}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "26fee794-b6c0-4662-9b6a-e758a81af724"],
 "."
}], "Notes",
 CellChangeTimes->{
