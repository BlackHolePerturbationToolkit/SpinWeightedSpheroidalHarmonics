Cell[CellGroupData[{

Cell["\<\
For s=0, SpinWeightedSpheroidalHarmonicS is related to SpheroidalPS\
\>", "MathCaption",
 CellChangeTimes->{3.8937541734626017`*^9},
 CellID->431612658,ExpressionUUID->"32238772-adea-4296-8849-f2bca9d36efd"],

Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"l", "=", "2"}], ",", 
     RowBox[{"m", "=", "2"}], ",", 
     RowBox[{"\[Gamma]", "=", "0.1"}], ",", 
     RowBox[{"\[Theta]", "=", 
      RowBox[{"\[Pi]", "/", "4"}]}]}], "}"}], ",", 
   RowBox[{
    RowBox[{"SpinWeightedSpheroidalHarmonicS", "[", 
     RowBox[{
     "0", ",", "l", ",", "m", ",", "\[Gamma]", ",", "\[Theta]", ",", "0"}], 
     "]"}], "==", 
    RowBox[{
     SqrtBox[
      FractionBox[
       RowBox[{
        RowBox[{"2", "l"}], "+", "1"}], 
       RowBox[{"4", "\[Pi]"}]]], 
     SqrtBox[
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"l", "-", "m"}], ")"}], "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"l", "+", "m"}], ")"}], "!"}]]], 
     RowBox[{"SpheroidalPS", "[", 
      RowBox[{"l", ",", "m", ",", 
       RowBox[{"I", " ", "\[Gamma]"}], ",", 
       RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "]"}]}]}]}], "]"}]], "Input",
 CellChangeTimes->{3.893754177999319*^9},
 CellID->2061667087,ExpressionUUID->"621aaadc-0529-4db3-ba0d-901757983273"]
}, Open  ]],

